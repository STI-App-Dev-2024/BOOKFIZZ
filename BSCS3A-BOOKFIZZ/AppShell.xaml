<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BSCS3A_BOOKFIZZ.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BSCS3A_BOOKFIZZ"
    Shell.FlyoutBehavior="Disabled"
    Shell.FlyoutItemIsVisible="False"
    Title="BSCS3A_BOOKFIZZ">

    <ShellContent
        Title="SignUp1"
        ContentTemplate="{DataTemplate local:SignUp1}"/>
    
    <ShellContent
        Title="LogIn"
        ContentTemplate="{DataTemplate local:LogIn}"  />

    <ShellContent
        Title="WelcomePage"
        ContentTemplate="{DataTemplate local:WelcomePage}"/>

    <ShellContent
        Title="SignUp2"
        ContentTemplate="{DataTemplate local:SignUp2}" />

    <ShellContent
    Title="MainPage"
    ContentTemplate="{DataTemplate local:MainPage}" />

    <ShellContent
        Title="Notification"
        ContentTemplate="{DataTemplate local:Notification}" />
    




</Shell>
